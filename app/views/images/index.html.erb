<h2>Images</h2>

<div class="breadcrumb">
  <%= link_to "regions", regions_path %> /
  <%= region_link(@region) %> /
  <%= link_to "images", region_images_path(@region) %>
</div>

<table class="table" id="images">
  <thead>
    <tr>
      <th>image_id</th>
      <th>name</th>
      <th>image_state</th>
    </tr>
  </thead>
  <tbody>
    <% @images.each do |image| %>
      <tr>
        <td><%= image_link(@region, image.image_id) %></td>
        <td><%= image.name %></td>
        <td><%= image.image_state %></td>
      </tr>
    <% end %>
  </tbody>
</table>
