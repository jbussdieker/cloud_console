<h2>Images</h2>

<div class="breadcrumb">
  <%= link_to "regions", regions_path %> /
  <%= region_link(@region) %> /
  <%= link_to "images", region_images_path(@region) %>
</div>

<table>
  <thead>
    <tr>
      <th>image_id</th>
      <th>name</th>
      <th>description</th>
      <th>image_state</th>
      <th>tag_set</th>
    </tr>
  </thead>
  <tbody>
    <% @images.each do |image| %>
      <tr>
        <td><%= image.image_id %></td>
        <td><%= image.name %></td>
        <td><%= image.description %></td>
        <td><%= image.image_state %></td>
        <td><%= image.tag_set %></td>
      </tr>
    <% end %>
  </tbody>
</table>
